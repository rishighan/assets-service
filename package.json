  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^5.10.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "moleculer-repl": "^0.5.3",
    "multer": "^1.4.2"
  },
  "dependencies": {
    "aws-sdk": "^2.441.0",
    "dotenv": "^8.0.0",
    "moleculer": "^0.13.0",
    "moleculer-web": "^0.8.0",
    "multer-s3": "^2.9.0",
    "nats": "^1.0.1"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
